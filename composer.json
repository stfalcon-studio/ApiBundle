{
    "name": "stfalcon-studio/api-bundle",
    "description": "Base classes and helper services to build API application via Symfony.",
    "keywords": ["bundle", "symfony", "api"],
    "homepage": "https://github.com/stfalcon-studio/ApiBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Community",
            "homepage": "https://github.com/stfalcon-studio/ApiBundle/graphs/contributors",
            "role": "Contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/stfalcon-studio/ApiBundle/issues"
    },
    "require": {
        "php": ">=8.1",
        "egulias/email-validator": "^3.2",
        "fresh/datetime": "^2.0",
        "ircmaxell/password-compat": "^v1.0.4",
        "justinrainbow/json-schema": "^5.2.12",
        "predis/predis": "^v1.1.10",
        "sentry/sentry-symfony": "^4.3.0",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/flex": "^2.2",
        "symfony/http-kernel": "^6.1",
        "symfony/http-foundation": "^6.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/serializer": "^6.1",
        "symfony/translation": "^6.1",
        "symfony/validator": "^6.1"
    },
    "require-dev": {
        "doctrine/common": "^3.3",
        "doctrine/mongodb-odm": "^2.4",
        "doctrine/orm": "^2.12",
        "escapestudios/symfony2-coding-standard": "^3.12",
        "gesdinet/jwt-refresh-token-bundle": "^v1.1.1",
        "jetbrains/phpstorm-attributes": "^1.0",
        "lexik/jwt-authentication-bundle": "^v2.15.1",
        "phpstan/phpstan": "^1.8.2",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.5",
        "slam/phpstan-extensions": "^6.0",
        "symfony/asset": "^6.1",
        "symfony/console": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/event-dispatcher": "^6.1",
        "symfony/filesystem": "^6.1",
        "symfony/form": "^6.1",
        "symfony/http-client": "^6.1",
        "symfony/messenger": "^6.1",
        "symfony/routing": "^6.1",
        "symfony/security-bundle": "^6.1",
        "symfony/security-core": "^6.1",
        "symfony/security-csrf": "^6.1",
        "symfony/workflow": "^6.1",
        "twig/twig": "^v3.4"
    },
    "autoload": {
        "psr-4": {
            "StfalconStudio\\ApiBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StfalconStudio\\ApiBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
