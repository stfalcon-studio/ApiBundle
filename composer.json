{
    "name": "stfalcon-studio/api-bundle",
    "description": "Base classes and helper services to build API application via Symfony.",
    "keywords": ["bundle", "symfony", "api"],
    "homepage": "https://github.com/stfalcon-studio/ApiBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/stfalcon-studio/ApiBundle/issues"
    },
    "require": {
        "php": ">=8.1",
        "doctrine/common": "^3.3",
        "doctrine/orm": "^2.12",
        "egulias/email-validator": "^3.2",
        "fresh/datetime": "^v2.0",
        "gesdinet/jwt-refresh-token-bundle": "^v1.1.1",
        "ircmaxell/password-compat": "^v1.0.4",
        "justinrainbow/json-schema": "^5.2.12",
        "lexik/jwt-authentication-bundle": "^v2.15.1",
        "predis/predis": "^v1.1.10",
        "sentry/sentry-symfony": "^4.3.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/flex": "^2.2",
        "symfony/http-kernel": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.12",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/phpstan": "^1.7.10",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "slam/phpstan-extensions": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/form": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/messenger": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/security-core": "^6.0",
        "symfony/security-csrf": "^6.0",
        "symfony/workflow": "^6.0",
        "twig/twig": "^v3.3"
    },
    "autoload": {
        "psr-4": {
            "StfalconStudio\\ApiBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StfalconStudio\\ApiBundle\\Tests\\": "Tests/"
        }
    }
}
